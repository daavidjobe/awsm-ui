{
  "name": "awsm-ui",
  "version": "0.0.1",
  "description": "A simplistic component library",
  "main": "./src/index.js",
  "scripts": {
    "start": "yarn storybook",
    "test": "yarn test:watch",
    "test:watch": "mocha --watch --require ignore-styles --require config/tests/setup.js src/**/*.spec.js --compilers config/tests/compiler",
    "test:once": "mocha --require ignore-styles --require config/tests/setup.js src/**/*.spec.js --compilers config/tests/compiler",
    "lint": "eslint src config docs/src docs/config --quiet --cache && echo \"eslint: no lint errors\"",
    "storybook": "start-storybook -p 3333",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "test:once"
  ],
  "repository": "git@bitbucket.org:netsolution/transportsedeln-web.git",
  "keywords": [],
  "author": "netSolution AB",
  "license": "MIT",
  "homepage": "https://bitbucket.org/daavidjobe/transportsedeln-web#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "app-module-path": "^2.2.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.0",
    "pre-commit": "^1.2.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "sass-loader": "^6.0.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-validator": "^2.3.0"
  }
}
