{
  "name": "transportsedeln-web",
  "version": "1.0.0",
  "description": "front-end for transportsedeln",
  "main": "index.js",
  "scripts": {
    "postinstall": "yarn run build",
    "dev": "webpack-dev-server --inline --hot  --config config/webpack.config.babel.js",
    "start": "yarn run dev",
    "build": "webpack -p --config config/webpack.config.babel.js --progress",
    "test": "yarn run test:watch",
    "test:watch": "mocha --watch --require ignore-styles --require config/tests/setup.js src/**/**/*.spec.js --compilers config/tests/compiler",
    "test:once": "mocha --require ignore-styles --require config/tests/setup.js src/**/**/*.spec.js --compilers config/tests/compiler",
    "lint": "eslint . --fix"
  },
  "pre-commit": [
    "test:once"
  ],
  "repository": "git@bitbucket.org:netsolution/transportsedeln-web.git",
  "keywords": [],
  "author": "netSolution AB",
  "license": "MIT",
  "homepage": "https://bitbucket.org/daavidjobe/transportsedeln-web#readme",
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "pre-commit": "^1.2.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-tap-event-plugin": "^2.0.1",
    "sinon": "^1.17.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-validator": "^2.3.0"
  }
}
